#!/usr/bin/python
from colorize import *

Run(Header(
    RegexpGroupsColors('^(PING )([^ ]*)( \\()([0-9.]+)(\\) .*)$', Underline, Yellow+Bold, White, Cyan+Bold, White),
    RegexpGroupsColors('^([0-9]+ bytes from .+(?: ?\\([0-9.]+\\))?: icmp_seq=)([0-9]+)( ttl=)([0-9]+)( time=)([0-9.]+ [a-z]+)$',
                       '', Red+Bold, '', Red, '', Green+Bold)))
